[
  load file {
    path: "example.json.gz" @ 0..0
  },
  decompress {
    type: "gzip" @ 0..0
  },
  read json {
    args: {
      builder_options: {
        policy: policy_precise {
          seed_schema: ""
        },
        settings: {
          default_name: "tenzir.json",
          ordered: true,
          expand_schema: false,
          raw: false,
          unnest_separator: "",
          timeout: "1s",
          desired_batch_size: 65536
        }
      },
      arrays_of_objects: false,
      mode: "none"
    }
  },
  write json {
    args: {
      
    }
  }
]