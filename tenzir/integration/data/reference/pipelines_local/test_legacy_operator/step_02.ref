[
  load file {
    path: "example.json.gz" @ 0..0
  },
  decompress {
    type: "gzip" @ 0..0,
    gzip_format: "gzip" @ 0..0
  },
  read json {
    parser_name: "json",
    builder_options: {
      policy: policy_default {
        
      },
      settings: {
        default_schema_name: "tenzir.json",
        ordered: true,
        schema_only: false,
        merge: false,
        raw: false,
        unnest_separator: "",
        timeout: "1s",
        desired_batch_size: 65536
      }
    },
    arrays_of_objects: false,
    mode: "none"
  },
  write json {
    tql: false
  }
]